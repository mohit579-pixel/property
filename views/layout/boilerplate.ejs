        <script>// open chat
        function openChat(){
          
          $(".chatBox").show(500);
          $(".chatBtn").addClass('hide');
          $(".chatBtn").removeClass('show');
        }


        function closeChat(){
          
          $(".chatBox").hide(500);
          $(".chatBtn").addClass('show');
          $(".chatBtn").removeClass('hide');
        }

        function sendNotification() {
        Push.create("Hello, World!", {
